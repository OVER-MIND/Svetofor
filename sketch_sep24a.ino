void setup()
{
  pinMode(10, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(1, OUTPUT);
}

  void loop ()
{
   digitalWrite (10, HIGH); 
  delay (1000); 
   digitalWrite (10, LOW);
  delay (1000);
  
   digitalWrite (5, HIGH); 
  delay (1000); 
   digitalWrite (5, LOW);
  delay (1000);
        
    digitalWrite (1, HIGH); 
  delay (1000); 
   digitalWrite (1, LOW);
  delay (1000);
  
   digitalWrite (5, HIGH); 
  delay (1000); 
   digitalWrite (5, LOW);
  delay (1000);
     

}
